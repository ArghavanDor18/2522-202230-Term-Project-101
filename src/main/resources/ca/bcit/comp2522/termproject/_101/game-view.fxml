<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>


<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" style="-fx-background-color: #4cbb17"
      fx:controller="ca.bcit.comp2522.termproject._101.GameController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="newGame"/>
    <Button text="Start a new game" onAction="#onNewGameButtonClick"/>
    <Label fx:id="instructions"/>
    <Button text="How To Play" onAction="#onInstructionsButtonClick"/>
</VBox>
